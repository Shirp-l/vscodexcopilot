{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "out",
    "lib": [
      "es6",
      "dom",
      "dom.iterable"],
    "sourceMap": true,
    "rootDir": ".",
    "skipDefaultLibCheck": true,
    "strict": true,
    "incremental": true,
    "noImplicitAny": false,
    "noUncheckedIndexedAccess": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", ".vscode-test", "test"]
}
